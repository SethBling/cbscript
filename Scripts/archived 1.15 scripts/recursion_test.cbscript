desc "Test for new data storage features"

import common

function recurse(x, y)
	if x >= 0
		tell @a '(x) (y)'
		recurse(x-1, y+1)
		tell @a '(x) (y)'
	end
end

function factorial(n)
	if n <= 1
		return 1
	else
		return factorial(n-1) * n
	end
end

reset
	recurse(5, 5)
	val = factorial(5)
	tell @a '(val)'
end
