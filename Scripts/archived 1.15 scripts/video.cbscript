clock rendering
	for @player in @a[rendering]
		@s.frame++
		frame = @s.frame
		as @Marker if @s.owner == @player._id and @s.frame == frame
			at @s rotated @s
				for $row in: range(48)
					for $col in: range(48)
						$left: col - 22.5
						$up: row - 22.5
						step = 0
						/setblock 0 1 0 barrier
						/setblock 0 3 0 barrier
						facing ^$left ^$up ^30
							ray_trace()
						end
						get_converted_block()
						/clone 0 3 0 0 3 0 $col $row 2
					end
				end
			end

			/kill @s
		end

		as create @Marker
			@s.move_to_coordinates()
			at @s
				/clone 0 0 2 47 47 2 ~ ~ ~
			end
			/kill @s
		end

		if @s.frame == @s.end_frame
			@s.rendering = False
			tell @s: {GDone rendering! \[[play](/function video:play)\]
		end
	end
end

function play()
	@s.playing = True
	@s.frame = 0
	@s.frame_step = 0
	/tp @s 24 225 30 180 0
end

clock playing
	for @player in @a[playing]
		@s.frame_step++
		if @s.frame_step == 2
			@s.frame_step = 0
			@s.frame++
			frame = @s.frame

			as create @Marker
				@s.move_to_coordinates()
				at @s
					/clone ~ ~ ~ ~47 ~47 ~ 0 201 0
				end
				/kill @s
			end

			if @s.frame == @s.end_frame
				@s.playing = False
				tell @s: {GDone playing!
			end
		end
	end
end
