define @Entity = @e
	x = Pos[0] double 1000
	y = Pos[1] double 1000
	z = Pos[2] double 1000
	ry = Rotation[0] float 1
	rx = Rotation[1] float 1
end

define @Marker = @Entity[type=area_effect_cloud,name=Marker]
	create: {"CustomName":"\"Marker\"", Duration:-1,Age:-2147483648,WaitTime:-2147483648}

	function move_to_coordinates()
		rem = frame
		y = rem / 128
		rem %= 128

		z = rem - 64

		y *= 64000
		z *= 1000

		@s.x = -64000
		@s.y = y
		@s.z = z
	end
end

